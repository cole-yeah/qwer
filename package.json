{
  "name": "qwer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "ssr": "node server.js",
    "build:all": "rimraf dist && npm run build:csr && npm run build:ssr",
    "dev:ssr": "cross-env RENDER_ENV=server node server.js",
    "start:ssr": "cross-env RENDER_ENV=server NODE_ENV=production node server.js",
    "build:ssr": "cross-env RENDER_ENV=server vue-cli-service build --dest dist/server",
    "build:csr": "cross-env RENDER_ENV=client vue-cli-service build --dest dist/client",
    "rebuild:docker": "npm run rm:docker && npm run build:docker",
    "rm:docker": "docker stop qwer && docker rm qwer && docker rmi qwer:v1.0.0",
    "build:docker": "docker build -t qwer:v1.0.0 .",
    "dev:docker": "docker run -itd --name qwer -p 8089:8089 qwer:v1.0.0"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.0.4",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "vant": "3.0.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0",
    "webpack": "^4.44.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0-0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "prettier": "^1.19.1",
    "serialize-javascript": "^5.0.1",
    "vue-bundle-renderer": "^0.2.2",
    "vue-loader-v16": "^16.0.0-beta.5.4",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.2.1"
  }
}
